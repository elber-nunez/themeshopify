  `img` varchar(100) DEFAULT NULL,
  `link` varchar(100) DEFAULT NULL,
  `content` text,
  PRIMARY KEY (`id_extra_banner`,`id_lang`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8;

