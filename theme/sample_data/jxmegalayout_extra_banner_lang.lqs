DROP TABLE IF EXISTS `ps_jxmegalayout_extra_banner_lang`;
CREATE TABLE `ps_jxmegalayout_extra_banner_lang` (
  `id_extra_banner` int(11) NOT NULL AUTO_INCREMENT,
  `id_lang` int(11) NOT NULL,
